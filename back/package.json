{
  "name": "back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon -L ./src/server.ts",
    "start:homologation": "cross-env NODE_ENV=homologation node build/server.js",
    "start:production": "cross-env NODE_ENV=production node build/server.js",
    "build": "cross-env NODE_ENV=development tsc -p .",
    "build:homologation": "cross-env NODE_ENV=homologation tsc -p .",
    "build:production": "cross-env NODE_ENV=production tsc -p .",
    "test": "cross-env NODE_ENV=test jest --clearCache && jest --no-cache --detectOpenHandles --forceExit",
    "test:docker": "cross-env NODE_ENV=test jest --clearCache && jest --no-cache --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=development jest --watch --no-cache --detectOpenHandles",
    "coverage": "cross-env NODE_ENV=development jest --coverage --no-cache --detectOpenHandles",
    "lint": "cross-env NODE_ENV=development eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json . --ext .ts",
    "typeorm": "cross-env NODE_ENV=development typeorm-ts-node-commonjs",
    "migration:show": "npm run typeorm -- migration:show -d ./src/database/internalDatabase.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./src/database/internalDatabase.ts",
    "migration:run:docker": "cross-env NODE_ENV=test typeorm-ts-node-commonjs -d ./src/database/internalDatabase.ts migration:run",
    "migration:run:homologation": "cross-env NODE_ENV=homologation typeorm-ts-node-commonjs -d ./src/database/internalDatabase.ts migration:run",
    "migration:run:production": "cross-env NODE_ENV=production typeorm-ts-node-commonjs -d ./src/database/internalDatabase.ts migration:run",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/database/internalDatabase.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "dockerBuild": "docker build -f Dockerfile.prod -t flirtz/teste:latest . && docker push flirtz/teste:latest"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.8",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.7.6",
    "@types/nodemailer": "^6.4.6",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.19",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  },
  "keywords": [],
  "author": "Matheus Contassot",
  "license": "ISC"
}
